<?xml version="1.0" encoding="utf-8"?><LinearLayout android:orientation="vertical"    android:layout_width="match_parent"    android:layout_marginTop="5dp"    android:layout_marginBottom="5dp"    android:layout_marginLeft="10dp"    android:layout_marginRight="10dp"    android:layout_height="wrap_content"    xmlns:android="http://schemas.android.com/apk/res/android">    <ImageView        android:id="@+id/imagen"        android:scaleType="fitXY"        android:src="@drawable/iconodeapp"        android:layout_width="match_parent"        android:layout_height="140dp"        />    <LinearLayout        android:layout_width="match_parent"        android:orientation="vertical"        android:layout_height="wrap_content">        <AutoCompleteTextView            android:layout_marginTop="2dp"            android:id="@+id/atvdni"            android:inputType="date"            android:layout_width="match_parent"            android:layout_height="40dp"            android:layout_marginBottom="10dp"            android:background="@drawable/edittext_background"            android:drawableLeft="@drawable/icon_person"            android:drawablePadding="10dp"            android:drawableStart="@drawable/icon_person"            android:hint="Ingrese fecha inicio"            android:paddingEnd="10dp"            android:paddingStart="10dp"            android:textColor="@color/black"            android:textColorHint="@color/black"            android:textSize="15sp" />        <AutoCompleteTextView            android:id="@+id/atvUsernameReg"            android:layout_width="match_parent"            android:layout_height="40dp"            android:layout_marginBottom="10dp"            android:background="@drawable/edittext_background"            android:drawableLeft="@drawable/icon_person"            android:drawablePadding="10dp"            android:drawableStart="@drawable/icon_person"            android:hint="Ingrese fecha fin "            android:paddingEnd="10dp"            android:inputType="date"            android:paddingStart="10dp"            android:textColor="@color/black"            android:textColorHint="@color/black"            android:textSize="15sp" />        <AutoCompleteTextView            android:inputType="time"            android:background="@drawable/edittext_background"            android:id="@+id/atvApp"            android:layout_width="match_parent"            android:layout_height="40dp"            android:layout_marginBottom="10dp"            android:drawableLeft="@drawable/icon_person"            android:drawablePadding="10dp"            android:drawableStart="@drawable/icon_person"            android:hint="Hora inicio"            android:paddingEnd="10dp"            android:paddingStart="10dp"            android:textColor="@color/black"            android:textColorHint="@color/black"            android:textSize="15sp" />        <AutoCompleteTextView            android:inputType="time"            android:background="@drawable/edittext_background"            android:id="@+id/atvApm"            android:layout_width="match_parent"            android:layout_height="40dp"            android:layout_marginBottom="10dp"            android:drawableLeft="@drawable/icon_person"            android:drawablePadding="10dp"            android:drawableStart="@drawable/icon_person"            android:hint="Hora fin"            android:paddingEnd="10dp"            android:paddingStart="10dp"            android:textColor="@color/black"            android:textColorHint="@color/black"            android:textSize="15sp" />        <AutoCompleteTextView            android:inputType="textAutoComplete"            android:layout_width="match_parent"            android:layout_height="40dp"            android:layout_marginBottom="10dp"            android:id="@+id/atvdirec"            android:background="@drawable/edittext_background"            android:drawableLeft="@drawable/icon_person"            android:drawablePadding="10dp"            android:drawableStart="@drawable/icon_person"            android:hint="Nombre de la cancha"            android:paddingEnd="10dp"            android:paddingStart="10dp"            android:textColor="@color/black"            android:textColorHint="@color/black"            android:textSize="15sp" />        <AutoCompleteTextView            android:inputType="textAutoComplete"            android:id="@+id/atvTelefono"            android:layout_width="match_parent"            android:layout_height="40dp"            android:layout_marginBottom="10dp"            android:background="@drawable/edittext_background"            android:drawableLeft="@drawable/icon_person"            android:drawablePadding="10dp"            android:drawableStart="@drawable/icon_person"            android:hint="Tipo de pago "            android:paddingEnd="10dp"            android:paddingStart="10dp"            android:textColor="@color/black"            android:textColorHint="@color/black"            android:textSize="15sp" />        <Button            android:id="@+id/btnReservar"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginTop="1dp"            android:background="@drawable/btnsignup_gradient"            android:text="Vender"            android:layout_marginBottom="10dp"            android:textColor="#FFFFFF" />    </LinearLayout></LinearLayout>